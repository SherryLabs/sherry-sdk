{
  "name": "@sherrylinks/sdk",
  "version": "2.19.3-beta.1",
  "description": "SDK for Sherry Links",
  "main": "dist/index.js",
  "module": "dist/index.esm.js",
  "browser": {
    "crypto": false,
    "node:crypto": false,
    "@noble/hashes/cryptoNode": "@noble/hashes/cryptoBrowser"
  },
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": {
        "browser": "./dist/sherry-sdk.browser.js",
        "default": "./dist/index.esm.js"
      },
      "require": {
        "browser": "./dist/sherry-sdk.browser.js",
        "default": "./dist/index.js"
      }
    }
  },
  "types": "dist/index.d.ts",
  "author": "Sherry Labs",
  "license": "MIT",
  "scripts": {
    "build": "tsc",
    "build:esm": "tsc --module ES2020 --outDir dist/esm && mv dist/esm/src/index.js dist/index.esm.js && rm -rf dist/esm",
    "build:browser": "webpack --mode production",
    "build:all": "npm run build && npm run build:esm && npm run build:browser",
    "prepublishOnly": "npm run build:all",
    "test": "jest",
    "test:coverage": "jest --coverage",
    "lint": "oxlint",
    "prepare": "husky",
    "publish": "npm run build:all && npm publish --access public",
    "format": "prettier --write .",
    "docs:start": "cd docs && yarn start",
    "docs:build": "cd docs && yarn build"
  },
  "files": [
    "dist"
  ],
  "devDependencies": {
    "@babel/core": "^7.26.0",
    "@babel/preset-env": "^7.26.0",
    "@babel/preset-typescript": "^7.26.0",
    "@types/jest": "^29.5.14",
    "babel-jest": "^29.7.0",
    "buffer": "^6.0.3",
    "crypto-browserify": "^3.12.1",
    "husky": "^9.1.7",
    "jest": "^27.4.5",
    "node-fetch": "^2.6.9",
    "oxlint": "^0.14.0",
    "prettier": "^3.5.3",
    "stream-browserify": "^3.0.0",
    "ts-jest": "^29.3.2",
    "ts-loader": "^9.5.2",
    "vm-browserify": "^1.1.2",
    "webpack": "^5.99.5",
    "webpack-cli": "^6.0.1"
  },
  "dependencies": {
    "@jest/globals": "^29.7.0",
    "abitype": "^1.0.6",
    "oxc-parser": "^0.38.0",
    "typescript": "^5.6.3",
    "viem": "^2.21.45"
  },
  "directories": {
    "test": "test"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/SherryLabs/sherry-sdk.git"
  },
  "bugs": {
    "url": "https://github.com/SherryLabs/sherry-sdk/issues"
  },
  "homepage": "https://github.com/SherryLabs/sherry-sdk#readme",
  "packageManager": "yarn@1.22.22+sha1.ac34549e6aa8e7ead463a7407e1c7390f61a6610"
}
