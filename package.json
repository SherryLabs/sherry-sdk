{
  "name": "@sherrylinks/sdk",
  "version": "0.10.0-beta.1",
  "description": "SDK for Sherry Links",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "author": "Sherry Labs",
  "license": "MIT",
  "scripts": {
    "build": "tsc",
    "prepublishOnly": "npm run build",
    "test": "jest",
    "test:coverage": "jest --coverage",
    "lint": "oxlint",
    "prepare": "husky",
    "publish": "npm run build && npm publish --access public",
    "format": "prettier --write ."
  },
  "files": [
    "dist"
  ],
  "devDependencies": {
    "@babel/core": "^7.26.0",
    "@babel/preset-env": "^7.26.0",
    "@babel/preset-typescript": "^7.26.0",
    "babel-jest": "^29.7.0",
    "husky": "^9.1.7",
    "jest": "^27.4.5",
    "node-fetch": "^2.6.9",
    "oxlint": "^0.14.0",
    "prettier": "^3.5.3"
  },
  "dependencies": {
    "@jest/globals": "^29.7.0",
    "abitype": "^1.0.6",
    "oxc-parser": "^0.38.0",
    "typescript": "^5.6.3",
    "viem": "^2.21.45"
  },
  "directories": {
    "test": "test"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/SherryLabs/sherry-sdk.git"
  },
  "bugs": {
    "url": "https://github.com/SherryLabs/sherry-sdk/issues"
  },
  "homepage": "https://github.com/SherryLabs/sherry-sdk#readme",
  "jest": {
    "preset": "ts-jest",
    "testEnvironment": "node",
    "collectCoverageFrom": [
      "src/**/*.{ts,tsx}",
      "!src/**/*.d.ts",
      "!src/**/index.ts"
    ],
    "coverageReporters": [
      "text",
      "lcov",
      "html"
    ],
    "setupFiles": [
      "./tests/setup.js"
    ]
  }
}
